{
  "version": 2,
  "buildCommand": "npm run build:vercel",
  "builds": [
    {
      "src": "vercel-entry.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["dist/**", "prisma/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/vercel-entry.js"
    }
  ]
}
